// Inspired by Catnap
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "kitty-direct",
        "source": "~/.config/assets/raiden0.png",
        "preserveAspectRatio": true,
        "width": 16,
        "height": 10,
        "padding": {
            "top": 1,
            "right": 2
            
            
        }
    },
    "general": {
    "multithreading": true
    

    },
    "display": {
        "separator":" ",
        "color":"magenta"
        
        
        

        
    },
    
    "modules": [
        {
            "key": "╔═══════════════╗",
            "type": "custom"
            
        },
        {
            "key": "║ {#31}👑 Master     {#keys}║",
            "type": "title",
            "format": "{user-name}",
            
            
            
        },
        {
            "key": "║ {#32}⌛ Session    {#keys}║",
            "type": "uptime"
            

        },
        {
            "key": "║ {#33}🐧 OS         {#keys}║",
            "type": "os"
            
            
        },
        
        {
            "key": "║ {#34}⚙️ Kernel     {#keys}║",
            "type": "kernel"
            
        },
        
        {
            "key": "║ {#35}📟 Terminal   {#keys}║",
            "type": "terminal"
        
        },
        {
            "key": "║ {#36}🐚 Shell      {#keys}║",
            "type": "shell"
            
        },
        {
            "key": "║ {#31}💻 CPU        {#keys}║",
            "type": "cpu",
            "showPeCoreCount": true,
            
            "temp":true
            
            
        },
        {
            "key": "║ {#32}🖥️ GPU        {#keys}║",
            "type": "gpu",
            "temp":true
            
            
        },
        {
            "key": "║ {#33}💾 Disk       {#keys}║",
            "type": "disk",
            "folders": "/"
            
        },
        {
            "key": "║ {#34}💽 RAM        {#keys}║",
            "type": "memory"
            
        },
        {
            "key": "║ {#35}📀 Swap       {#keys}║",
            "type": "swap"
            
        },
        {
            "type": "datetime",
            "key": "║ {#36}⏰ Time       {#keys}║",
            "format": "{14}:{17}:{20} / {1}-{3}-{11}"
        },
        
        {
            "key": "╚═══════════════╝",
            "type": "custom"
        }
    ]
}
